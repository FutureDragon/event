<div class="page-header">
  <h1>Neuste Musik</h1>
</div>
<table class="table table-hover">
  <thead>
    <tr>
      <th>Name</th>
    </tr>
  </thead>

  <tbody>
    <% @musics.each do |music| %>
      <tr>
        <td><%= link_to music.name , music %></td>
      </tr>
    <% end %>
  </tbody>
</table>

